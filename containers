package containers;

public class RefrigeratedContainer extends HeavyContainer {
	public RefrigeratedContainer(int ID, int weight) {
		super(ID, weight);
	}
	final double fuelConsumption = 4.00; protected int portID;protected int shipID;
	public double consumption() {
		return fuelConsumption*weight;
	}
	boolean equals(Container other) {
		if(weight == other.weight && ID == other.ID) {
			return true;
		} else {
			return false;
		}
	}
	public int getPortID() {
		return portID;
	}

	public void setPortID(int portID) {
		this.portID = portID;
	}

	public int getShipID() {
		return shipID;
	}

	public void setShipID(int shipID) {
		this.shipID = shipID;
	}

}



