package containers;

public class LiquidContainer extends HeavyContainer{
	public LiquidContainer(int ID, int weight) {
		super(ID, weight);
	}
	final double fuelConsumption = 5.00; protected int portID;protected int shipID;
	public double consumption() {
		return fuelConsumption*weight;
	}
	boolean equals(Container other) {
		if(weight == other.weight && ID == other.ID) {
			return true;
		} else {
			return false;
		}
	}
	public int getPortID() {
		return portID;
	}

	public void setPortID(int portID) {
		this.portID = portID;
	}

	public int getShipID() {
		return shipID;
	}

	public void setShipID(int shipID) {
		this.shipID = shipID;
	}

}



